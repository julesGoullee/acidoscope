{
  "name": "acidoscope",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dll": "vue-cli-service dll",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./src/assets/logo.png --output=build --flatten",
    "linkproxy": "node ./src/abletonLinkProxy",
    "rebuild:electron": "electron-rebuild -f -w sharp",
    "rebuild:node": "npm rebuild",
    "test:e2e": "vue-cli-service test:e2e",
    "test:e2e:headless": "yarn test:e2e --headless",
    "test:unit": "NODE_ENV=test vue-cli-service test:unit -r tests/unit/test.config.js"
  },
  "main": "background.js",
  "dependencies": {
    "@vue/cli-service": "^3.5.3",
    "abletonlink": "0.0.7",
    "socket.io": "^2.2.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.3",
    "@vue/cli-plugin-e2e-cypress": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-mocha": "^3.5.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "electron-icon-builder": "^1.0.0",
    "electron-rebuild": "^1.8.4",
    "electron": "^4.1.2",
    "express": "^4.16.4",
    "eslint": "^5.15.3",
    "eslint-plugin-vue": "^5.2.2",
    "lodash": "^4.17.11",
    "nosleep.js": "^0.9.0",
    "sinon": "^7.3.1",
    "socket.io-client": "^2.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "three": "^0.103.0",
    "vue-analytics": "^5.16.4",
    "vue-cli-plugin-dll": "^1.1.6",
    "vue-cli-plugin-electron-builder": "^1.2.0",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "^1.5.7",
    "vuetify-loader": "^1.2.1",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0",
    "webmidi": "^2.3.3",
    "webpack-glsl-loader": "^1.0.1"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=8.0.0"
  }
}
