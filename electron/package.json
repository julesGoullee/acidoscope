{
  "name": "shaders-midi-electron",
  "productName": "Shaders midi",
  "version": "0.1.0",
  "main": "main.js",
  "bin": "cli.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "test": "mocha",
    "package": "npm-run-all package:*",
    "rebuild:electron": "electron-rebuild -f -w sharp",
    "rebuild:node": "npm rebuild",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:sign-exe": "signcode './out/ShadersMidi-win32-ia32/ShadersMidi.exe' --cert ~/ShadersMidi.p12 --prompt --name 'ShadersMidi' --url 'http://electron.atom.io'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/ShadersMidiSetup.exe' --cert ~/ShadersMidi.p12 --prompt --name 'ShadersMidi' --url 'http://electron.atom.io'",
    "xpackage:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks"
  },
  "repository": "https://github.com/julesGoullee/shaders-midi",
  "devDependencies": {
    "@octokit/rest": "^16.3.2",
    "electron": "^4.0.1",
    "check-for-leaks": "^1.2.0",
    "devtron": "^1.3.0",
    "electron-packager": "^12.1.0",
    "electron-rebuild": "^1.8.4",
    "electron-winstaller": "^2.2.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.5.2",
    "signcode": "^0.5.0",
    "spectron": "^5.0.0"
  },
  "dependencies": {
    "abletonlink": "^0.0.7",
    "electron-log": "^2.2.14",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "express": "^4.16.4",
    "highlight.js": "^9.3.0",
    "socket.io": "^2.2.0",
    "update-electron-app": "^1.1.1"
  },
  "engines": {
    "node": "10.x"
  }
}
